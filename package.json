{
  "name": "@warp-ds/vue",
  "repository": "git@github.com:warp-ds/vue.git",
  "version": "1.2.4-next.2",
  "description": "Warp components for Vue 3",
  "type": "module",
  "exports": {
    ".": "./dist/warp-vue.js",
    "./docs": "./dist/docs/warp-vue.js"
  },
  "imports": {
    "#util": "./components/util/index.js",
    "#components": "./index.js",
    "#generics": "./components/generic/index.js",
    "#dev-util": "./dev/src/util.js"
  },
  "scripts": {
    "build-storybook": "storybook build",
    "build": "vite build --mode lib && vite build --mode eik",
    "commit": "cz",
    "dev": "vite dev dev --config ./vite.config.js",
    "messages:compile": "lingui compile",
    "messages:extract": "lingui extract",
    "postsite": "cd dev/dist && cp index.html 200.html && cp index.html 404.html",
    "site": "vite build dev --config ./vite.config.js",
    "storybook": "storybook dev -p 6006",
    "test": "vitest run --coverage",
    "watch": "vitest watch"
  },
  "keywords": [],
  "license": "Apache-2.0",
  "dependencies": {
    "@floating-ui/dom": "^1.5.4",
    "@lingui/core": "^4.7.0",
    "@warp-ds/core": "^1.0.2",
    "@warp-ds/css": "^1.7.0",
    "@warp-ds/icons": "2.0.0",
    "@warp-ds/uno": "^1.6.0",
    "create-v-model": "^2.2.0",
    "dom-focus-lock": "^1.1.0",
    "element-collapse": "^1.1.0",
    "femtobounce": "^1.0.0",
    "scroll-doctor": "^2.0.0"
  },
  "devDependencies": {
    "@eik/cli": "^2.0.22",
    "@eik/rollup-plugin": "^4.0.60",
    "@itsy/token": "^2.0.0-next.0",
    "@lingui/cli": "^4.7.0",
    "@lingui/conf": "^4.7.0",
    "@lingui/extractor-vue": "^4.7.0",
    "@lukeed/uuid": "^2.0.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-actions": "^7.6.10",
    "@storybook/addon-essentials": "^7.6.10",
    "@storybook/addon-interactions": "^7.6.10",
    "@storybook/addon-links": "^7.6.10",
    "@storybook/blocks": "^7.6.10",
    "@storybook/builder-vite": "^7.6.10",
    "@storybook/cli": "^7.6.10",
    "@storybook/test": "^7.6.10",
    "@storybook/test-runner": "^0.16.0",
    "@storybook/vue3": "^7.6.10",
    "@storybook/vue3-vite": "^7.6.10",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/coverage-v8": "^1.2.2",
    "@vue/compiler-sfc": "^3.4.14",
    "@vue/test-utils": "^2.4.1",
    "cleave-lite": "^1.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "drnm": "^0.9.0",
    "esbuild": "^0.19.11",
    "fuse.js": "^7.0.0",
    "happy-dom": "^13.0.0",
    "playwright": "^1.41.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semantic-release": "^23.0.0",
    "shikiji": "^0.9.0",
    "storybook": "^7.6.10",
    "unocss": "^0.58.0",
    "vite": "^5.0.0",
    "viteik": "^1.0.3",
    "vitest": "^1.2.1",
    "vue": "^3.4.14",
    "vue-router": "^4.2.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eik": {
    "server": "https://assets.finn.no",
    "files": "./dist",
    "import-map": "https://assets.finn.no/map/finn/v2"
  }
}
